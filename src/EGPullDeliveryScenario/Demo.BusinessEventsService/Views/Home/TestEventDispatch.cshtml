@model List<TransactionEvent.TransactionEventData>

@{
    ViewData["Title"] = "Test Transaction Events";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <table>
        <tr>
            <th>Id</th>
            <th>Client Id</th>
            <th>Amount</th>
            <th>Transaction Date Time</th>            
        </tr>
        @foreach (var tran in Model)
        {
            <tr>
                <td>@tran.Transactionid</td>
                <td>@tran.Clientid</td>
                <td>@tran.TransactionDateTime.ToDateTime()</td>

            </tr>
        }
    </table>
</div>
